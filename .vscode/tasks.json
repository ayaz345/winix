{
    "tasks": [
        {
            "type": "shell",
            "label": "GCC Build fsutil",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-Werror",
                "-Wimplicit-fallthrough",
                "-Wsequence-point",
                "-Wswitch-default",
                "-Wswitch-unreachable",
                "-Wswitch-enum",
                "-Wstringop-truncation",
                "-Wbool-compare",
                "-Wtautological-compare",
                "-Wfloat-equal",
                "-Wshadow=global",
                "-Wpointer-arith",
                "-Wpointer-compare",
                "-Wcast-align",
                "-Wcast-qual",
                "-Wwrite-strings",
                "-Wdangling-else",
                "-Wlogical-op",
                "-Wunused",
                "-Wpointer-to-int-cast",
                "-Wno-discarded-qualifiers",
                "-DFSUTIL",
                "-D_DEBUG",
                "-I${workspaceFolder}/include/fs_include",
                "-I${workspaceFolder}/include",
                "${workspaceFolder}/fs/*.c",
                "${workspaceFolder}/fs/system/*.c",
                "${workspaceFolder}/fs/fsutil/*.c",
                "${workspaceFolder}/fs/mock/*.c",
                "${workspaceFolder}/winix/bitmap.c",
                "-o",
                "${workspaceFolder}/fsutil"
            ],
            "options": {
                "cwd": "/usr/bin"
            },
        },
        {
            "type": "shell",
            "label": "GCC Build unittest",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-Werror",
                "-Wimplicit-fallthrough",
                "-Wsequence-point",
                "-Wswitch-default",
                "-Wswitch-unreachable",
                "-Wswitch-enum",
                "-Wstringop-truncation",
                "-Wbool-compare",
                "-Wtautological-compare",
                "-Wfloat-equal",
                "-Wshadow=global",
                "-Wpointer-arith",
                "-Wpointer-compare",
                "-Wcast-align",
                "-Wcast-qual",
                "-Wwrite-strings",
                "-Wdangling-else",
                "-Wlogical-op",
                "-Wunused",
                "-Wpointer-to-int-cast",
                "-Wno-discarded-qualifiers",
                "-DFSUTIL",
                "-D_DEBUG",
                "-I${workspaceFolder}/include/fs_include",
                "-I${workspaceFolder}/include",
                "${workspaceFolder}/fs/*.c",
                "${workspaceFolder}/fs/system/*.c",
                "${workspaceFolder}/fs/mock/*.c",
                "${workspaceFolder}/winix/bitmap.c",
                "${workspaceFolder}/tests/*.c",
                "-o",
                "${workspaceFolder}/unittest"
            ],
            "options": {
                "cwd": "/usr/bin"
            },
        },
        {
            "label": "python utest_runner.c",
            "type": "shell",
            "command": "python3",
            "args": [
                "tools/utest_generator.py",
                "tests/*.c",
                ">",
                "tests/utest_runner.c"
            ]
        },
        {
            "label": "Build Unit Test",
            "dependsOn":[
                "python utest_runner.c",
                "GCC Build unittest"
            ]
        }
    ],
    "version": "2.0.0"
}